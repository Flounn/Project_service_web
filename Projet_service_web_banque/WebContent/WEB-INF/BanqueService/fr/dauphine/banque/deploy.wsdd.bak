<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from BanqueService WSDL service -->

  <service name="Banque" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://banque.dauphine.fr"/>
      <parameter name="wsdlServiceElement" value="BanqueService"/>
      <parameter name="schemaQualified" value="http://banque.dauphine.fr"/>
      <parameter name="wsdlServicePort" value="Banque"/>
      <parameter name="className" value="fr.dauphine.banque.BanqueSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Banque"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="consultDevise" qname="operNS:consultDevise" xmlns:operNS="http://banque.dauphine.fr" returnQName="retNS:consultDeviseReturn" xmlns:retNS="http://banque.dauphine.fr" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:email" xmlns:pns="http://banque.dauphine.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:mdp" xmlns:pns="http://banque.dauphine.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="consultSolde" qname="operNS:consultSolde" xmlns:operNS="http://banque.dauphine.fr" returnQName="retNS:consultSoldeReturn" xmlns:retNS="http://banque.dauphine.fr" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:email" xmlns:pns="http://banque.dauphine.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:mdp" xmlns:pns="http://banque.dauphine.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="retraitEur" qname="operNS:retraitEur" xmlns:operNS="http://banque.dauphine.fr" returnQName="retNS:retraitEurReturn" xmlns:retNS="http://banque.dauphine.fr" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:email" xmlns:pns="http://banque.dauphine.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:mdp" xmlns:pns="http://banque.dauphine.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:montant" xmlns:pns="http://banque.dauphine.fr" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="depot" qname="operNS:depot" xmlns:operNS="http://banque.dauphine.fr" returnQName="retNS:depotReturn" xmlns:retNS="http://banque.dauphine.fr" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:email" xmlns:pns="http://banque.dauphine.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:mdp" xmlns:pns="http://banque.dauphine.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:montant" xmlns:pns="http://banque.dauphine.fr" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="delCompte" qname="operNS:delCompte" xmlns:operNS="http://banque.dauphine.fr" returnQName="retNS:delCompteReturn" xmlns:retNS="http://banque.dauphine.fr" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:email" xmlns:pns="http://banque.dauphine.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:mdp" xmlns:pns="http://banque.dauphine.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getCompte" qname="operNS:getCompte" xmlns:operNS="http://banque.dauphine.fr" returnQName="retNS:getCompteReturn" xmlns:retNS="http://banque.dauphine.fr" returnType="rtns:Compte" xmlns:rtns="http://banque.dauphine.fr" soapAction="" >
        <parameter qname="pns:email" xmlns:pns="http://banque.dauphine.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:mdp" xmlns:pns="http://banque.dauphine.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="addCompte" qname="operNS:addCompte" xmlns:operNS="http://banque.dauphine.fr" returnQName="retNS:addCompteReturn" xmlns:retNS="http://banque.dauphine.fr" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:nom" xmlns:pns="http://banque.dauphine.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:prenom" xmlns:pns="http://banque.dauphine.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:email" xmlns:pns="http://banque.dauphine.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:mdp" xmlns:pns="http://banque.dauphine.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:devise" xmlns:pns="http://banque.dauphine.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="retraitEur consultDevise getCompte depot addCompte consultSolde delCompte"/>

      <typeMapping
        xmlns:ns="http://banque.dauphine.fr"
        qname="ns:Compte"
        type="java:fr.dauphine.banque.Compte"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
